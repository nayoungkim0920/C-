<OpenCV에서 CUDA설정하기>
cmake -B build -S .. -G "Visual Studio 17 2022" -A x64 -D WITH_CUDA=ON -D CUDA_ARCH_BIN=8.6 -D CUDA_ARCH_PTX="" -D OPENCV_EXTRA_MODULES_PATH=C:/opencv_contrib/modules -D CMAKE_BUILD_TYPE=Debug -D CMAKE_INSTALL_PREFIX=C:/opencv/sources/build/install
cmake -B build -S .. -G "Visual Studio 17 2022" -A x64 -D WITH_CUDA=ON -D CUDA_ARCH_BIN=8.6 -D CUDA_ARCH_PTX="" -D OPENCV_EXTRA_MODULES_PATH=C:/opencv_contrib/modules -D CMAKE_BUILD_TYPE=Release -D CMAKE_INSTALL_PREFIX=C:/opencv/sources/build/install
